    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "deepmerge": "^4.3.1",
    "discord.js": "^14.16.1",
    "form-data": "^4.0.0",
    "husky": "^9.0.11",
    "icecast-metadata-js": "^1.2.8",
    "icecast-parser": "^4.0.2",
    "icy": "^2.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lint-staged": "^15.2.5",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nestjs-cls": "^4.3.0",
    "nestjs-dynamic-providers": "^0.3.4",
    "nestjs-pino": "^4.1.0",
    "number-format.js": "^2.0.9",
    "pg": "^8.11.5",
    "pino-http": "^10.1.0",
    "postgres": "^3.4.4",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "require-yml": "^2.0.0",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.13",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "commitizen": "^4.3.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "app/commitizen.js"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings 0"
    ]
  }
}
